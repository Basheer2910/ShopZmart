{
    "manifest_version": 3,
    "name": "ShopZmart",
    "description": "Extension to compare prices of products from different E-Commerce websites.",
    "version": "1.0.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ]
}